{
  "name": "typoteka",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "test": "npm run eslint && npm run jest",
    "jest": "jest",
    "eslint": "eslint --ext .js src/",
    "server": "cross-env LOG_LEVEL=error node -r dotenv/config ./src/service/service.js --server 3000",
    "start::debug": "cross-env LOG_LEVEL=info NODE_ENV=development node -r dotenv/config ./src/service/service.js",
    "start-front-server": "node -r dotenv/config ./src/express/index.js",
    "start": "concurrently --names \"BACKEND,SSR\" \"npm:server\" \"npm:start-front-server\""
  },
  "author": "Poly Titova",
  "license": "ISC",
  "devDependencies": {
    "eslint": "7.2.0",
    "eslint-config-htmlacademy": "0.5.2",
    "jest": "26.6.3",
    "nodemon": "2.0.4",
    "pino-pretty": "4.3.0",
    "sqlite3": "5.0.2",
    "supertest": "6.0.1"
  },
  "dependencies": {
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "chalk": "4.1.0",
    "concurrently": "6.2.0",
    "connect-session-sequelize": "7.1.1",
    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "express-session": "1.17.2",
    "joi": "17.4.0",
    "multer": "1.4.2",
    "nanoid": "3.1.16",
    "pg": "8.6.0",
